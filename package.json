{
  "name": "@blockforfun/cli",
  "version": "1.0.1",
  "description": "BlockFor.fun registry CLI",
  "main": "index.js",
  "oclif": {
    "commands": "./commands",
    "bin": "blockfun",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "bin": {
    "blockfun": "./bin/run"
  },
  "scripts": {
    "release": "standard-version",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockforfun/registry.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/bin",
    "/commands",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "author": "Mikael Karon <mikael@karon.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blockforfun/registry/issues"
  },
  "homepage": "https://github.com/blockforfun/registry#readme",
  "dependencies": {
    "@es-git/core": "^0.9.0",
    "@es-git/fetch-mixin": "^0.9.0",
    "@es-git/load-as-mixin": "^0.9.0",
    "@es-git/memory-repo": "^0.9.0",
    "@es-git/mix": "^0.9.0",
    "@es-git/node-fs-repo": "^0.9.0",
    "@es-git/object-mixin": "^0.9.0",
    "@es-git/walkers-mixin": "^0.9.0",
    "@es-git/zlib-mixin": "^0.9.0",
    "@oclif/command": "^1.5.6",
    "@oclif/config": "^1.9.0",
    "@oclif/plugin-help": "^2.1.4",
    "micromatch": "^3.1.10",
    "node-fetch": "^2.3.0",
    "sqlite": "^3.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.4",
    "babel-runtime": "^6.26.0",
    "eslint": "^5.9.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.1",
    "rimraf": "^2.6.2",
    "standard-version": "^4.4.0"
  }
}
