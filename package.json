{
  "name": "@blockforfun/cli",
  "version": "5.0.1",
  "description": "BlockFor.fun CLI",
  "main": "index.js",
  "oclif": {
    "scope": "blockforfun",
    "bin": "blockfun",
    "plugins": [
      "@oclif/plugin-help",
      "@blockforfun/plugin-git",
      "@blockforfun/plugin-build"
    ]
  },
  "bin": {
    "blockfun": "./bin/run"
  },
  "scripts": {
    "release": "lerna publish --conventional-commits",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockforfun/cli.git"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/plugins",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "author": "Mikael Karon <mikael@karon.se>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blockforfun/cli/issues"
  },
  "homepage": "https://blockfor.fun/cli",
  "dependencies": {
    "@blockforfun/plugin-build": "^3.0.1",
    "@blockforfun/plugin-git": "^1.1.2",
    "@oclif/command": "^1.5.6",
    "@oclif/config": "^1.9.0",
    "@oclif/plugin-help": "^2.1.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.19.4",
    "eslint": "^5.9.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^8.0.1",
    "lerna": "^3.4.3",
    "rimraf": "^2.6.2"
  }
}
